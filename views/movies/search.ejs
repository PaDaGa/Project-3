<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head')  %>
  </head>
  <body>
    <ul>
      <% results.forEach(function(movie){  %>
        <li><a href="movies/<%= movie.id %>"><%= movie.title %></a></li>
        <p> <%= movie.rating %> | <%= movie.release_date %> </p>
      <% }) %>
    </ul>
  </body>
</html>
